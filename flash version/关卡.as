function BrickGenerate(row:int,col:Array,type:Array):void
{
	var brick1:brick
	for(var i=0;i<col.length;i++)
	{
		for(var j=0;j<col[i].length;j++)
		{
			brick1 = new brick(type[i].concat())
			brick1.y=(BRICK_HEIGHT) * (row+0.5)
			brick1.x=(BRICK_WIDTH) * (col[i][j]+0.5)
			bricksprite.addChild(brick1)
		}
	}
}


